
item_list = record {
    items: list<string>;
}

sort_order = enum {
    ascending;
    descending;
    random;
}

sort_items = interface +c {
    sort(order: sort_order, items: item_list);
    static new(listener: items_listener): sort_items;
}

items_listener = interface +j +o {
    update(items: item_list);
}